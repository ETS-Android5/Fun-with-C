<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="420dp"
    android:layout_height="match_parent"
    android:background="@drawable/onlda"
    tools:context=".Online_Resources">

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:layout_marginBottom="643dp"
        android:background="@drawable/ad"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="20dp"
            android:fontFamily="@font/roboto_black"

            android:shadowColor="#000000"
            android:shadowDx="8.5"
            android:shadowDy="8.3"
            android:shadowRadius="13.6"
            android:text="Online Resources"
            android:textColor="#FABF0E"
            android:textSize="39dp"
            android:textStyle="bold" />
    </LinearLayout>

    <TextView
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="20dp"
        android:backgroundTint="#6AFF0101"
        android:text="GATE Preparation:"
        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_marginVertical="200dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <HorizontalScrollView

            android:layout_width="match_parent"
            android:layout_height="130dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="130dp"
                android:orientation="horizontal"
                android:layout_marginStart="3dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/geeksfor"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/geeksforgeeks"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/gflow"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/gateoverflow"
                        android:scaleType="fitXY"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/examside"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/examside"
                        android:scaleType="fitXY"/>

                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/raudra"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/raudra"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/baijuc"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="fitXY"
                        android:src="@drawable/baijuce"/>
                </androidx.cardview.widget.CardView>






            </LinearLayout>

        </HorizontalScrollView>
    </LinearLayout>


    <TextView
        android:id="@+id/textView2"
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="513dp"
        android:backgroundTint="#6AFF0101"
        android:text="Interview Preparation:"

        android:textSize="30dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <HorizontalScrollView
            android:id="@+id/horizontalScrollView"
            android:layout_width="match_parent"
            android:layout_height="130dp">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="130dp"
                android:orientation="horizontal"
                android:layout_marginStart="3dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/ibit"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/ib"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/javatpoint"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/javatpoint"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/tutorialspoint"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/tutorialspoint"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/edureka"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/edureka"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/hackrio"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/hackr"/>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/quescol"
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    app:cardCornerRadius="30dp"
                    android:layout_marginLeft="10dp">

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/quescol"/>
                </androidx.cardview.widget.CardView>





            </LinearLayout>

        </HorizontalScrollView>
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>